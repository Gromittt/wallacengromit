#include <iostream>
#include <cstdlib>
#include <ctime>
using namespace std;

int main(void) {
	int random, number_m, number_s;
	srand(time(NULL));

	random = rand() % 100;

	cout << "랜덤한 수: " << random << endl;
	cout << "특정 배수 개수: ";
	cin >> number_m ;

	for (int i = 1; i < number_m + 1; i++) {
		cout <<"\n\n" << i << "번째 특정 배수: ";
		cin >> number_s;
		cout << "<특정 배수만 출력하는 프로그램>" << endl;
		for (int j = 1; j <= random; j++) {
			if (j % number_s != 0)
				continue;
			cout << j << " ";
		}
		cout << "\n<특정 배수 제외 출력하는 프로그램>"<< endl;
		for (int q = 1; q <= random; q++) {
			if (q % number_s == 0)
				continue;
			cout << q << " ";
		}

	}
}
